<view><form><swiper class="swiper-box" style="{{'height:'+(swiperHeight)+';'}}" current="{{subjectIndex}}" data-event-opts="{{[['change',[['SwiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{subjectList}}" wx:for-item="subject" wx:for-index="index" wx:key="index"><swiper-item><block wx:if="{{index-subjectIndex>=-1&&index-subjectIndex<=1}}"><view><view class="cu-bar bg-white solid-bottom"><view class="action text-black"><text class="cuIcon-title text-red"></text>{{subject.title+''}}</view></view><view><block wx:if="{{subject.type===1||subject.type===3}}"><radio-group data-event-opts="{{[['change',[['RadioboxChange',['$event']]]]]}}" class="block" bindchange="__e"><block wx:for="{{subject.optionList}}" wx:for-item="option" wx:for-index="index" wx:key="index"><view class="cu-form-group flex" style="margin-top:1rem;"><radio value="{{option.id}}"></radio><view class="title text-black">{{option.id+"."+option.content}}</view></view></block></radio-group></block></view></view></block></swiper-item></block></swiper></form><view class="notice"><view>用户须知:</view><view><text style="font-size:15px;">{{"可以联系售后微信:"+wx}}</text></view><view><text style="font-size:15px;">{{"可以拨打售后电话:"+tel}}</text></view></view><view class="commit_bt"><button type="primary" data-event-opts="{{[['tap',[['withdraw']]]]}}" bindtap="__e">申请退货</button></view></view>