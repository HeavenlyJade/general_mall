.center_box_bg {
  background: #F9F9F9;
  position: relative;
}
.center_box_bg .profily {
  position: absolute;
  width: 90%;
  margin: 0 auto;
  top: -100rpx;
  left: 5%;
  background: #FEFEFE;
  padding: 35rpx;
  box-sizing: border-box;
  box-shadow: 0px 2px 5px #EDEDED;
}
.base {
  display: flex;
  align-items: center;
  border-bottom: 2px solid #F6F6F6;
  padding-bottom: 35rpx;
  position: relative;
}
.base .profily_header {
  height: 120rpx;
  width: 120rpx;
  background-image: url(data:image/jpeg;base64,);
  background-size: 100%;
}
.base text {
  margin-left: 20px;
  font-size: 30rpx;
}
.base image {
  position: absolute;
  height: 40rpx;
  width: 40rpx;
  right: 0px;
  top: 0px;
}
.order_status {
  display: flex;
  justify-content: space-between;
  margin-top: 35rpx;
}
.order_status .status {
  width: 140rpx;
  font-size: 24rpx;
  text-align: center;
  letter-spacing: 0.5px;
  display: flex;
  flex-direction: column;
}
.order_status .status .icon {
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
  margin-bottom: 5px;
}
.baiban {
  background: #FEFEFE;
  height: 300rpx;
}
.center_menu {
  width: 100%;
  display: inline-block;
}
.center_menu .menu_item {
  font-size: 28rpx;
  letter-spacing: 1px;
  padding: 14px 5%;
  background: #FEFEFE;
  overflow: hidden;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  position: relative;
  border-bottom: 1px solid #EFEFEF;
}
.center_menu .menu_item:hover {
  background: #F6F6F6 !important;
}
.center_menu .menu_item::after {
  content: "";
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 5%;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.center_menu .menu_item text:nth-of-type(1) {
  margin-left: 10px;
}
.center_menu .menu_item image {
  width: 40rpx;
  height: 40rpx;
}
.center_menu .menu_item:nth-of-type(4) {
  margin-top: 10px;
}
